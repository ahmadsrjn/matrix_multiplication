
<div>
    <div class="d-flex justify-content-evenly fixed-top mt-2">
        <button class="btn btn-danger progress-bar-striped">
            <%= link_to  'Go Back', root_path, method: :get, class: "text-decoration-none text-white fw-bolder" %>
        </button>
    </div>
    <h2>Result Matrix</h2>
    <% if @result_matrix %>
    <% @result_matrix.each do |row| %>
        <div>
        <% row.each do |value| %>
            <%= value %> 
        <% end %>
        </div>
    <% end %>
    <% else %>
    <p>No matrix calculated yet.</p>
    <% end %>
</div>